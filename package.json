{
  "name": "dk-typeorm-spcall",
  "version": "1.0.2",
  "description": "Aim: Providing easy functionality for call SPs using typescript mainly for nestjs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "typeorm",
    "mssql",
    "storage procedure",
    "dk-typeorm-spcall"
  ],
  "author": "Dhaval Kamaliya",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KamaliyaDhawal/dk-typeorm-spcall"
  },
  "bugs": {
    "url": "https://github.com/KamaliyaDhawal/dk-typeorm-spcall/issues"
  },
  "homepage": "https://github.com/KamaliyaDhawal/dk-typeorm-spcall#readme",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "prettier": "^2.7.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "mssql": "^9.0.1",
    "typeorm": "^0.3.10"
  }
}
